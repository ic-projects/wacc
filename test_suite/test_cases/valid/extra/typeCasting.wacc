# spooky type casting

# Output:
# 135228
# <
# true
# 4
# 
# true

# Program:

begin
	pair(string, pair) s = newpair("behold", null) ;
	pair(pair, pair) n = newpair(s, null) ;

	pair(int, pair) casted = fst n ;
	int nowInt = fst casted ;
	println nowInt ;

	pair(char, pair) casted2 = fst n ;
	char nowChar = fst casted2 ;
	println nowChar ;

	pair(bool, pair) casted3 = fst n ;
	bool nowBool = fst casted3 ;
	println nowBool ;


	pair(int, pair) ss = newpair(4, null) ;
	n = newpair(ss, null) ;

	casted = fst n ;
	nowInt = fst casted ;
	println nowInt ;

	casted2 = fst n ;
	nowChar = fst casted2 ;
	println nowChar ;

	casted3 = fst n ;
	nowBool = fst casted3 ;
	println nowBool
end
