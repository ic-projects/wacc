# basic array printing

# Output:
# Printing an array variable gives an address, such as #addrs#

# Program:

begin
  int[] y = [1, 2, 3];
  char[] z = ['a', 'b', 'c'];
  pair(int[], char[]) x = newpair(y, z);
  println y ;
  println z ;
  println z[2] ;
  char[] a = snd x ;
  println a;
  println a[0] ;
  println a[1] ;
  println a[2] ;
  int[] b = fst x ;
  println b[0] ;
  println y[0] ;
  println b[1] ;
  println y[1] ;
  println b[2] ;
  println y[2]
end
