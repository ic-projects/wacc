# structs

# Output:
# #empty#

# Exit:
# 0

# Program:

struct Node is
  lhs ;
  rhs ;
  value ;
end

begin
  printTree(n) is
    if n != null
    then
      print "[" ;
      print n.value ;
      print " -> " ;
      x = call printTree(n.lhs) ;
      x = call printTree(n.rhs) ;
      print "]"
    else
      skip
    fi ;
    return 0
  end

  swap(n) is
    temp = n.lhs ;
    n.lhs = n.rhs ;
    n.rhs = temp ;
    return 0
  end

  i = new Node(null, null, 1) ;
  i2 = new Node(null, null, 2) ;
  i3 = new Node(i, i2, 3) ;
  i4 = new Node(i3, null, 4) ;
  i5 = new Node(null, null, 5) ;
  i6 = new Node(null, null, 6) ;
  i7 = new Node(i6, i5, 7) ;
  i8 = new Node(i7, i4, 8) ;
  x = call printTree(i8) ;
  println x ;
  i9 = call swap(i8) ;
  println "=========" ;
  x = call printTree(i8) ;
  println x ;

  exit 0
end
